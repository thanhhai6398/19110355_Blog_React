\n        setBlog(blog);\r\n    }, [])\r\n    const navigate = useNavigate();\r\n    const handleSubmit = (e) => {\r\n        e.preventDefault();\r\n        const answer = window.confirm('Are you sure you want to save blog');\r\n        if (answer) {\r\n            setBlogs(prev => {\r\n                const temp = [...prev];\r\n                const index = prev.findIndex(b => b.id === Number.parseInt(id));\r\n                temp[index] = blog;\r\n                return temp;\r\n            })\r\n            navigate('/')\r\n        }\r\n\r\n    }\r\n    const handleChange = (e) => {\r\n        setBlog(prev => {\r\n            return { ...prev, [e.target.name]: e.target.value };\r\n        })\r\n    }\r\n    return (\r\n        <div className='form-wrapper'>\r\n            <h3>Edit Blog</h3>\r\n            <form onSubmit={handleSubmit}>\r\n                <input\r\n                    type='text'\r\n                    placeholder='Title'\r\n                    className='form-control'\r\n                    name='title'\r\n                    value={blog.title}\r\n                    onChange={handleChange} />\r\n                <textarea\r\n                    placeholder='Enter content'\r\n                    value={blog.content}\r\n                    name='content'\r\n                    onChange={handleChange}></textarea>\r\n                <button>Save</button>\r\n            </form>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default EditBlog","D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\blogItem.js",[],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\models\\blogModel.js",[],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\AddBlog.js",[],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\BlogDetail.js",["115"],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\ListBlogs.js",[],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\EditBlog.js",["116"],[],"D:\\HK7\\CCNPMM\\BT\\blog_reactjs\\src\\components\\BlogItem.js",[],[],"D:\\HK